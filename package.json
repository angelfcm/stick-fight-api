{
  "name": "prize-ranking",
  "version": "1.0.0",
  "description": "API for Quizz Fight game",
  "main": ".build/index.js",
  "bin": {
    "tslint": "./bin/tslint"
  },
  "scripts": {
    "start": "serverless offline start --env dev --lib dom",
    "test": "ts-node node_modules/mocha/bin/mocha --reporter spec --colors --env dev",
    "watch": "tsc -w",
    "lint": "tslint 'src/**/*.ts'",
    "prettier-fix": "prettier 'src/**/*.ts'",
    "seed-dev": "ts-node src/db/seeders/seed --env dev --reset",
    "deploy-dev": "serverless deploy --env dev"
  },
  "dependencies": {
    "aws-sdk": "^2.520.0",
    "busboy": "^0.3.1",
    "custom-env": "^1.0.2",
    "faker": "^4.1.0",
    "mongoose": "^5.6.11",
    "serverless-dotenv-plugin": "^2.1.1",
    "uuid": "^3.3.3",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/busboy": "^0.2.3",
    "@types/faker": "^4.1.5",
    "@types/lodash": "^4.14.138",
    "@types/mongoose": "^5.5.16",
    "@types/node": "^12.7.3",
    "@types/uuid": "^3.4.5",
    "@types/validator": "^10.11.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "install": "^0.13.0",
    "npm": "^6.13.4",
    "prettier": "1.18.2",
    "serverless-offline": "^5.10.1",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-node": "^8.3.0",
    "tslint": "~5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angelfcm/prize-ranking.git"
  },
  "author": "Angel Carriola",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/angelfcm/prize-ranking/issues"
  },
  "homepage": "https://github.com/angelfcm/prize-ranking#readme"
